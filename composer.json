{
  "name": "cutwise/oauth-server-bundle",
  "type": "symfony-bundle",
  "description": "Symfony2 OAuth Server Bundle",
  "keywords": [
    "oauth",
    "oauth2",
    "server"
  ],
  "homepage": "http://friendsofsymfony.github.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Arnaud Le Blanc",
      "email": "arnaud.lb@gmail.com"
    },
    {
      "name": "FriendsOfSymfony Community",
      "homepage": "https://github.com/FriendsOfSymfony/FOSOAuthServerBundle/contributors"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "friendsofsymfony/oauth2-php": "~1.3",
    "symfony/framework-bundle": "^5.1",
    "symfony/security-bundle": "^5.1",
    "symfony/dependency-injection": "^5.1",
    "ext-json": "*"
  },
  "require-dev": {
    "symfony/yaml": "^5.1",
    "symfony/form": "^5.1",
    "symfony/console": "^5.1",
    "symfony/twig-bundle": "^5.1",
    "doctrine/doctrine-bundle": "~2.0",
    "doctrine/orm": "~2.8",
    "phpunit/phpunit": "~9.0",
    "symfony/phpunit-bridge": "^5.1",
    "php-mock/php-mock-phpunit": "^2.6"
  },
  "suggest": {
    "doctrine/doctrine-bundle": "*",
    "symfony/form": "Needed to be able to use the AuthorizeFormType",
    "symfony/console": "Needed to be able to use commands"
  },
  "autoload": {
    "psr-4": {
      "FOS\\OAuthServerBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.5-dev"
    }
  }
}
