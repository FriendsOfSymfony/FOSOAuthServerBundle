{
    "name": "friendsofsymfony/oauth-server-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 OAuth Server Bundle",
    "keywords": ["oauth", "oauth2", "server"],
    "homepage": "http://friendsofsymfony.github.com",
    "authors": [
        {
          "name": "Arnaud Le Blanc",
          "email": "arnaud.lb@gmail.com"
        },
        {
            "name": "FriendsOfSymfony Community",
            "homepage": "https://github.com/FriendsOfSymfony/FOSOAuthServerBundle/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "friendsofsymfony/oauth2-php": "1.0.*",
        "symfony/framework-bundle": "2.0.*",
        "symfony/security-bundle": "2.0.*"
    },
    "require-dev": {
        "symfony/symfony": "dev-master",
        "doctrine/orm": "dev-master",
        "doctrine/mongodb-odm": "dev-master",
        "willdurand/propel-typehintable-behavior": "dev-master"
    },
    "suggest": {
        "doctrine/doctrine-bundle": "2.0.*",
        "doctrine/mongodb-odm-bundle": "2.0.*",
        "propel/propel-bundle": "If you want to user Propel with Symfony2, then you will have to install the PropelBundle",
        "willdurand/propel-typehintable-behavior": "The Typehintable behavior is useful to add type hints on generated methods, to be compliant with interfaces"
    },
    "autoload": {
        "psr-0": { "FOS\\OAuthServerBundle": "" }
    },
    "target-dir": "FOS/OAuthServerBundle"
}
