{
    "name": "lutskanu/oauth-server-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 OAuth Server Bundle",
    "keywords": ["oauth", "oauth2", "server"],
    "homepage": "http://friendsofsymfony.github.com",
    "license": "MIT",
    "authors": [
        {
          "name": "Arnaud Le Blanc",
          "email": "arnaud.lb@gmail.com"
        },
        {
            "name": "FriendsOfSymfony Community",
            "homepage": "https://github.com/FriendsOfSymfony/FOSOAuthServerBundle/contributors"
        }
    ],
    "require": {
        "php": "^5.3.3|^7.0",
        "lutskanu/oauth2-php": "~1.3",
        "symfony/framework-bundle": "~2.2|~3.0",
        "symfony/security-bundle": "~2.1|~3.0",
        "symfony/dependency-injection": "^2.0.5|~3.0"
    },
    "require-dev": {
        "symfony/class-loader": "~2.1|~3.0",
        "symfony/yaml": "~2.1|~3.0",
        "symfony/form": "~2.3|~3.0",
        "symfony/console": "~2.1|~3.0",
        "doctrine/doctrine-bundle": "~1.0",
        "doctrine/orm": "~2.2"
    },
    "suggest": {
        "doctrine/doctrine-bundle": "*"
    },
    "autoload": {
        "psr-4": { "FOS\\OAuthServerBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.6-dev"
        }
    }
}
