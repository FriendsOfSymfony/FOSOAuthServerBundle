{
    "name": "lutskanu/oauth-server-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 OAuth Server Bundle",
    "keywords": ["oauth", "oauth2", "server"],
    "homepage": "http://friendsofsymfony.github.com",
    "license": "MIT",
    "authors": [
        {
          "name": "Arnaud Le Blanc",
          "email": "arnaud.lb@gmail.com"
        },
        {
            "name": "FriendsOfSymfony Community",
            "homepage": "https://github.com/FriendsOfSymfony/FOSOAuthServerBundle/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "lutskanu/oauth2-php": "~1.2",
        "symfony/framework-bundle": "~2.2",
        "symfony/security-bundle": "~2.1"
    },
    "require-dev": {
        "symfony/class-loader": "~2.1",
        "symfony/yaml": "~2.1",
        "doctrine/doctrine-bundle": "~1.0",
        "doctrine/orm": ">=2.2,<2.5-dev"
    },
    "suggest": {
        "doctrine/doctrine-bundle": "*"
    },
    "autoload": {
        "psr-0": { "FOS\\OAuthServerBundle": "" }
    },
    "target-dir": "FOS/OAuthServerBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.5-dev"
        }
    }
}
